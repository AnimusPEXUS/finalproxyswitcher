{

  "manifest_version": 2,
  "name": "Final Proxy Switcher",
  "version": "0.1",

  "description": "Tool for Better Security",

  "icons": {},

  "permissions": [
    "<all_urls>",
    "storage",
    "downloads",
    "proxy",
    "notifications",
    "webRequest",
    "webRequestBlocking"
  ],

  "background": {
    "scripts": [
      "wasm_exec.js",
      "background.js"
    ]
  },

  "browser_action": {
    "default_icon": {

    },
    "default_title": "Whereami?"
  },

  "content_security_policy": "script-src 'self' 'sha256-qznLcsROx4GACP2dm0UCKCzCG+HiZ1guq6ZZDob/Tng='; object-src 'self'"

}
